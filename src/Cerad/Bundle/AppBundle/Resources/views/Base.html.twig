  <script src="/s1games/bundles/ceradapp/js/droplinemenu.js" type="text/javascript"></script>
  <script type="text/javascript">
    //build menu with DIV ID="myslidemenu" on page:
    droplinemenu.buildmenu("droplinetabs1")
  </script>
  <script type="text/javascript" charset="utf8" src="/s1games/bundles/ceradapp/js/ceradcore.js"></script>

{% block stylesheets %}{%endblock %}

{% if cerad_tourn_show('wordpress') %}
  {% set referer = cerad_tourn_get_referer() %}
  <script src="http://ayso1ref.com/wp-content/themes/section1/js/zayso.js"   type="text/javascript"></script>
  <link  href="http://ayso1ref.com/wp-content/themes/section1/css/zayso.css" type="text/css" rel="stylesheet" media="all" />
{% endif %}

</head>

<body>
{% block layout_body %}
  {% block layout_header   %}{%endblock %}
  {% block layout_topmenu  %}{%endblock %}
  {% block layout_userinfo %}{%endblock %}
  {% block layout_content  %}{%endblock %}
{%endblock %}

{% block javascripts %}{% endblock %}
