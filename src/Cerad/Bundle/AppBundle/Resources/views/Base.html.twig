  <meta name="purpose" content="Development styling"/>

  <link rel="stylesheet" type="text/css" href="/s1games/bundles/ceradapp/css/reset.css" media="all" />
  <link rel="stylesheet" type="text/css" href="/s1games/bundles/ceradapp/css/style.css" media="all" />
  <link rel="stylesheet" type="text/css" href="/s1games/bundles/ceradapp/css/common.css" media="all" />
  <link rel="stylesheet" type="text/css" href="/s1games/bundles/ceradapp/css/app.css" media="all" />
  <link rel="stylesheet" type="text/css" href="/s1games/bundles/ceradapp/css/s1.css" media="all" />
