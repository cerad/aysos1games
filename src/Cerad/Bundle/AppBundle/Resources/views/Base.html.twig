  <link rel="stylesheet" type="text/css" href="/s1games/bundles/ceradapp/css/droplinebar.css" />
  <script src="http://ajax.aspnetcdn.com/ajax/jquery/jquery-1.8.3.min.js" type="text/javascript" charset="utf8"></script>
